{
  "name": "react-boilerplate-9000",
  "version": "1.0.0",
  "description": "the react boilerplate from the future",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaosaari/react-boilerplate-9000.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Janne Saari",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jaosaari/react-boilerplate-9000/issues"
  },
  "homepage": "https://github.com/jaosaari/react-boilerplate-9000#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.3"
  }
}
